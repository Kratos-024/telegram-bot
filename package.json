{
  "name": "matchmaking-bot",
  "version": "1.0.0",
  "description": "A simple matchmaking Telegram bot",
  "main": "dist/index.js",
  "scripts": {
    "build": "npm install && tsc -b",
    "start": "node dist/index.js",
    "dev": "npm run build && nodemon dist/index.js",
    "postinstall": "npm run prisma-generate",
    "prisma-migrate": "npx prisma migrate dev --schema=src/db/prisma/schema.prisma",
    "prisma-generate": "npx prisma generate --schema=src/db/prisma/schema.prisma",
    "prisma-studio": "npx prisma studio --schema=src/db/prisma/schema.prisma"
  },
  "dependencies": {
    "@prisma/client": "^6.10.1",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "node-cron": "^4.1.1",
    "node-telegram-bot-api": "^0.66.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.23",
    "@types/node": "^20.19.2",
    "@types/node-telegram-bot-api": "^0.64.9",
    "nodemon": "^3.1.10",
    "prisma": "^6.10.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "keywords": [
    "telegram-bot",
    "typescript",
    "express",
    "prisma",
    "matchmaking"
  ],
  "author": "Your Name",
  "license": "MIT"
}
